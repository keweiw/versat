<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="createfund-action" namespace="/employee/fund" extends="versat">
    	<action name="createfund" class="com.action.FundAction" method="createFund">
    		<result name="success">/pages/createfund.jsp</result>
    	</action>
        <action name="create" class="com.action.FundAction" method="create">
            <result name="error">/pages/createfund.jsp</result>
            <result name="success" type="redirect">/pages/createfund.jsp</result>
            <result name="input" type="redirect">/pages/createfund.jsp</result>
        </action>
    </package>
</struts>